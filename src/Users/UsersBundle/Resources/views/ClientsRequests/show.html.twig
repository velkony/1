{% extends "::layout/layout.html.twig" %}

{% block body -%}
    <h1>ClientsRequests</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Daterequest</th>
                <td>{{ entity.dateRequest|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Checkin</th>
                <td>{{ entity.checkIn|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Checkout</th>
                <td>{{ entity.checkOut|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Numbernights</th>
                <td>{{ entity.numberNights }}</td>
            </tr>
            <tr>
                <th>Hotelid</th>
                <td>{{ entity.hotelId }}</td>
            </tr>
            <tr>
                <th>Breakfast</th>
                <td>{{ entity.breakfast }}</td>
            </tr>
            <tr>
                <th>Lunch</th>
                <td>{{ entity.lunch }}</td>
            </tr>
            <tr>
                <th>Dinner</th>
                <td>{{ entity.dinner }}</td>
            </tr>
            <tr>
                <th>Surname</th>
                <td>{{ entity.surname }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Gsm</th>
                <td>{{ entity.gsm }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Fax</th>
                <td>{{ entity.fax }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('client_request') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('clientsrequests_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
